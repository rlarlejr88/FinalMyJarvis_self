parameterType="Meeting">
insert into tbl_meeting 
		(
		meeting_no,
		member_no,
		meet_title,
		meet_content,
		meet_date,
		gpt_summary
		)
	values
		(
		SEQ_MEETING_NO.NEXTVAL,
		#{memberNo},
		#{meetTitle},
		#{meetContent},
		#{meetDate},
		)
</insert>

